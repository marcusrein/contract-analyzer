{
	"name": "contract-analyzer",
	"version": "1.0.0",
	"description": "A utility to analyze smart contracts on Ethereum and other EVM-compatible blockchains",
	"type": "module",
	"main": "startBlock.js",
	"bin": {
		"cana": "./cli.js"
	},
	"scripts": {
		"start": "node startBlock.js",
		"test": "node test-local.js",
		"dev": "node test-local.js",
		"analyze": "node cli.js analyze",
		"chains:list": "node cli.js chains list",
		"chains:add": "node cli.js chains add",
		"setup": "node setup.js",
		"help": "node help.js",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"clean": "rm -rf node_modules dist build coverage"
	},
	"keywords": [
		"ethereum",
		"blockchain",
		"smart-contract",
		"block-number",
		"deployment",
		"ethers",
		"abi",
		"events",
		"verification",
		"subgraph",
		"cli",
		"evm",
		"web3"
	],
	"author": "Marcus Rein",
	"license": "MIT",
	"dependencies": {
		"chalk": "^5.4.1",
		"commander": "^11.1.0",
		"dotenv": "^16.3.1",
		"ethers": "^6.13.5",
		"nanospinner": "^1.1.0"
	},
	"devDependencies": {
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"lint-staged": "^15.2.0",
		"prettier": "^3.2.5"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/yourusername/contract-analyzer.git"
	},
	"bugs": {
		"url": "https://github.com/yourusername/contract-analyzer/issues"
	},
	"homepage": "https://github.com/yourusername/contract-analyzer#readme",
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		]
	}
}
